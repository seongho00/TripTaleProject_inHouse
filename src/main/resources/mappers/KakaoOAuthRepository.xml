<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.example.demo.repository.KakaoOAuthRepository">


	<select id="getKakaoMemberById" parameterType="map"
		resultType="com.example.demo.vo.KakaoMember">
		SELECT *
		FROM kakaoMember
		WHERE id = #{id}
	</select>
	
	<select id="doJoin" parameterType="map"
		resultType="com.example.demo.vo.KakaoMember">
		INSERT INTO kakaoMember
		SET id = #{id},
		regDate = #{regDate},
		updateDate = #{updateDate},
		nickname = #{nickname},
		email = #{email},
		profileImage = #{profileImage}
	</select>

</mapper>